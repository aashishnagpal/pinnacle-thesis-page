<header class="eui-grid__container">
  <div class="eui-grid__row">
    <div class="eui-grid__column eui-grid__col-size-12-of-12">
      <img class="logo-image" src="images/logo.png">
    </div>
  </div>

  {{> studentProfile }}

  <div class="eui-grid__row">
    <div class="eui-grid__column eui-grid__col-size-12-of-12">
      <h2 class="page-title">Pinnacle Project Thesis</h2>
    </div>
  </div>
</header>
<section class="eui-grid__container">
  {{> projectName }}
</section>
<section class="eui-grid__container thesis">
  {{> projectDetails content=description}}
  {{> projectDetails heading="Problem" content=problemSolving}}
  {{> projectDetails heading="Solution" content=solution}}
  {{> projectDetails heading="Project's Impact on Humanity" content=humanityImpact}}
</section>
<section class="eui-grid__container">
  {{> teamMembers}}
</section>

<section class="eui-grid__container join-team-form">
  {{> joinTeamForm}}
</section>
